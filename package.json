{
  "name": "stackathon",
  "version": "1.0.0",
  "description": "Individual project",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server & npm run sass",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "sass": "sass --watch ./public/scss/main.scss:./public/css/main.css",
    "test": "NODE_ENV=test JWT=secret_sugar mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch"
  },
  "keywords": [
    "stackathon"
  ],
  "author": "Savannah for Fullstack Academy",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "nodemon": "^2.0.16",
    "react-redux": "^8.0.1",
    "react-router-dom": "^5.3.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chalk": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "node-sass": "^7.0.1",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "sequelize": "^6.19.0"
  }
}
